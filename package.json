    "babel-preset-env": "^1.2.0",
    "babel-preset-flow": "^6.23.0",
    "coveralls": "^2.12.0",
    "cross-env": "^3.2.3",
    "eslint": "^3.17.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-compat": "^1.0.2",
    "eslint-plugin-flowtype": "^2.30.0",
